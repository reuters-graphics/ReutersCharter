<%var domain = self.colorScale.domain();
var range = self.colorScale.range();%>
<div class='legendContainer scatter'>
	<div class="legend-items-holder">
		<%domain.forEach( function (d,i)  {%>
			<div class="legendItems" data-id="<%=d%>">
				<div class='circleTip <%=self.chartType%>' style='background-color:<%= range[i]%>;'></div>
				<div class="legendInline">
					<div class="nameTip">	<%= d %></div>
					<div class='valueTip'>
					</div>
				</div>
			</div>
		<%})%>
	</div>		
</div>

