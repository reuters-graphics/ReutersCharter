<%if (data[0].quarters){%>
	<div class='dateTip'> <%=data[0].quarters%> <%=data[0].displayDate%> </div>
<%}else if (data[0].displayDate){%>
	<div class='dateTip'> <%=data[0].displayDate%> </div>
<%}else{%>
	<div class='dateTip'> <%=data[0].category%> </div>
<%}%>
<%if (!data[0].category && !data[0].displayDate){%>
	<div class='dateTip'> <%=data[0][this.xValue]%> </div>
<%}%>

<%data.forEach(function(d,i){%>
		<div class="tipHolder">
			<%if(data.length > 1){%>
				<div class='circleTip <%=this.chartType%> <%if (this.chartLayout == "outlineBar" && i==1){%>outline<%}%>' style='background-color:<% print(this.colorScale(d.name))%>;'></div>
				<div class='nameTip'><%=d.displayName%></div>
			<%}%>
			<div class='valueTip'>
				<%if (this.chartLayout == "stackPercent"){%>
					<%print(this.tipNumbFormat(d.y1Percent - d.y0Percent))%>				
				<%}else{%>
					<%print(this.tipNumbFormat(d[this.dataType]))%>				
				<%}%>
			</div>
	
		</div>
<%})%>	
<%if(this.timelineData){
	var timelineData = this.timelineDataGrouped[this.timelineDate(data[0].date)]
	print(this.timelineTemplate({data:timelineData, this:this}))
}%>	